<?xml version="1.0" encoding="UTF-8"?>
<com.jf_eam_project.imagetools.SquareLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <FrameLayout
        android:id="@+id/framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:descendantFocusability="blocksDescendants">

        <com.jf_eam_project.imagetools.LocalImageView
            android:id="@+id/child_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop" />

        <View
            android:id="@+id/view_ImageView_up"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#40000000"
            android:visibility="gone"></View>

        <!-- android:layout_width="@dimen/photo_history_detail_container_heigth"
                   android:layout_height="@dimen/photo_history_detail_container_heigth" -->
        <ImageView
            android:id="@+id/child_checkbox"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="right"
            android:layout_margin="5dp" />

        <!-- android:button="@drawable/pictures_select_icon" -->
    </FrameLayout>
</com.jf_eam_project.imagetools.SquareLayout>